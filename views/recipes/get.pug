doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Recipes
    body 
        each recipe in recipes
            h1= recipe.name
            h2 Procedure
            p= recipe.procedure