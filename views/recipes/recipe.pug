doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title name
    body 
        h1= name
        h2 Procedure
        p= procedure
        each photo in Photos
            img(src=`${process.env.PHOTO_PATH}${photo.url}`, alt=name)